{
  "name": "course-hugo-scss",
  "version": "0.0.1",
  "description": "Skillshare course - build scss with hugo",
  "main": "index.js",
  "scripts": {
    "dev": "hugo server --gc --disableFastRender",
    "dev:drafts": "hugo server --gc --disableFastRender --buildFuture --buildDrafts",
    "build": "hugo --gc --cleanDestinationDir --minify",
    "render:dev": "hugo server --gc --disableFastRender --renderToDisk --cleanDestinationDir",
    "render:prod": "hugo server --gc --disableFastRender --renderToDisk --cleanDestinationDir --environment production --minify",
    "npm-check": "npm-check -u"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/future-wd/course-hugo-bootstrap-scss.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/future-wd/course-hugo-bootstrap-scss/issues"
  },
  "homepage": "https://github.com/future-wd/course-hugo-bootstrap-scss#readme",
  "dependencies": {
    "bootstrap": "^5.1.3",
    "npm-check": "^5.9.2"
  }
}
